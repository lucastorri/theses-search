<div id="top" class="well form-search row">
  <div id="logo" class="span3">
    <h1>Theses Search</h1>
  </div>
  <div id="search-box" class="span3">
    <input id="query" type="text" name="query" autofocus="true" placeholder="search"/>
    <button id="search" class="btn btn-primary"><i class="icon-search icon-white"></i></button>
  </div>
</div>

<script id="template" type="text/x-jquery-tmpl"> 
  <div class="span12 hero-unit">
    <h2>${id}: </h2>
    <p>{{html matches}}</p>
  </div>
</script>
<div id="results" class="row" />

<script src="jquery-1.7.2.min.js"></script>
<script src="http://ajax.microsoft.com/ajax/jquery.templates/beta1/jquery.tmpl.min.js"></script>
<script src="index.js"></script>